<Page x:Class="OOPLAST9.EditFrame"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:OOPLAST9"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="EditFrame">

    <Grid>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="14,364,0,0" TextWrapping="Wrap"  VerticalAlignment="Top"  Width="254"  x:Name="UserLastLog"/>
        <Label Content="Login" HorizontalAlignment="Left" Margin="14,331,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="312,364,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" x:Name="UserNewPass" Width="291"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="312,331,0,0" VerticalAlignment="Top" />
        <Button Content="Edit" x:Name="SaveButton" HorizontalAlignment="Left" Margin="608,349,0,0" VerticalAlignment="Top" Width="160" Height="38" Click="SaveButton_Click"/>
        <DataGrid HorizontalAlignment="Left" Height="321" VerticalAlignment="Top" Width="790" Name="TableGrid"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="14,392,0,0" TextWrapping="Wrap" ToolTip="Enter new login" VerticalAlignment="Top" Name="UserNewLog" Width="254"/>

    </Grid>
</Page>
